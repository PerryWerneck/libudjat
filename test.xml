<?xml version="1.0" encoding="UTF-8" ?>

<config>

	<module name='http' required='no' />
	<module name='information' required='no' />
	<module name='civetweb' required='no' />

	<agent name='intvalue' type='integer' value='0'>

		<attribute name='summary' value='Summary of agent ${name}' />
		
		<agent name='subkey' type='integer' value='0' />
		<agent name='text' type='string' value='test-value' />

		<state name='sample' value='0' summary='The intvalue is 0' level='ready' />

	</agent>

	<random name='random600' update-timer='600'>

		<state name='s0' value='0' summary='Random 0'>
			<alert name='r0.0' url='http://localhost' />
		</state>
		
		<state name='s1' value='1' summary='Random 1' />
		<state name='s2' value='2' summary='Random 2' />
		<state name='s3' value='3' summary='Random 3' />
		<state name='s4' value='4' summary='Random 4' />
		<state name='s5' value='5' summary='Random 5' />
		
	</random>

	<random name='random10' update-timer='10'>

		<attribute name='url' value='http://localhost' />

		<state name='s0' value='0' summary='Random 0' alert='yes'>
		</state>

		<state name='s1' value='1' summary='Random 1' alert='yes'>
		</state>

		<state name='s2' value='2' summary='Random 2' alert='yes'>
		</state>

		<state name='s3' value='3' summary='Random 3' alert='yes'>
		</state>
		
		<state name='s4' value='4' summary='Random 4' alert='yes'>
		</state>
		
		<state name='s5' value='5' summary='Random 5' alert='yes'>
		</state>

	</random>

</config>
