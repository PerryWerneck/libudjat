prefix=@prefix@
exec_prefix=@exec_prefix@
includedir=${prefix}/@includedir@
libdir=${prefix}/@libdir@

version_major=@PACKAGE_VERSION_MAJOR@
version_minor=@PACKAGE_VERSION_MINOR@
version_micro=@PACKAGE_VERSION_MICRO@

product_name=@PRODUCT_NAME@
product_id=@PRODUCT_DOMAIN@.@PRODUCT_NAME@
product_title=@PRODUCT_TITLE@
product_version=@PACKAGE_VERSION_MAJOR@@PACKAGE_VERSION_MINOR@

build_id=@PACKAGE_CORE_BUILD@

package_name=lib@PRODUCT_NAME@@PACKAGE_VERSION_MAJOR@_@PACKAGE_VERSION_MINOR@
module_path=${libdir}/@PRODUCT_NAME@/@PACKAGE_VERSION_MAJOR@.@PACKAGE_VERSION_MINOR@/modules/
module_version=@PACKAGE_VERSION_MAJOR@.@PACKAGE_VERSION_MINOR@

Name: @PACKAGE_NAME@
Description: @PACKAGE_DESCRIPTION@
Version: @PACKAGE_VERSION@
Libs: @DYNLIBS@
Cflags: -I${includedir}/@PACKAGE_NAME@

